# Database configuration
DATABASE__URL=postgresql+asyncpg://username:password@localhost:5432/database_name

# Telegram bot token (get from @BotFather on Telegram)
TELEGRAM__BOT_TOKEN=your_telegram_bot_token_here

# Security
SECURITY__SECRET_KEY=your_super_secret_key_here

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOGGING_LEVEL=INFO

# Optional: Telegram webhook configuration
# TELEGRAM__WEBHOOK_URL=https://your-domain.com/webhook
# TELEGRAM__WEBHOOK_SECRET=your_webhook_secret_here

# Optional: Redis URL for caching
# REDIS__URL=redis://localhost:6379/0

# Optional: Admin Telegram IDs (comma-separated)
# ADMIN__SUPER_ADMIN_TELEGRAM_IDS=123456789,987654321
